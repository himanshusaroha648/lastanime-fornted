{
  "name": "aniverse",
  "type": "javascript",
  "build": {
    "command": "npm run build",
    "cwd": "aniVerse1",
    "watch_paths": ["src/**/*.{js,jsx,ts,tsx}"]
  },
  "env": {
    "production": {
      "build": {
        "command": "npm run build",
        "cwd": "aniVerse1"
      },
      "vars": {
        "ENVIRONMENT": "production"
      }
    }
  },
  "pages_build_output_dir": "dist",
  "compatibility_date": "2024-12-01",
  "compatibility_flags": ["nodejs_compat"]
}
